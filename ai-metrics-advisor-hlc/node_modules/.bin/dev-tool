#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/marygao/project/azure-sdk-for-js/common/tools/dev-tool/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/common/tools/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/common/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/node_modules:/mnt/c/Users/marygao/project/node_modules:/mnt/c/Users/marygao/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/sdk/metricsadvisor/ai-metrics-advisor/node_modules/@azure/dev-tool/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/sdk/metricsadvisor/ai-metrics-advisor/node_modules/@azure/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/sdk/metricsadvisor/ai-metrics-advisor/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/sdk/metricsadvisor/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/sdk/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/marygao/project/azure-sdk-for-js/common/tools/dev-tool/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/common/tools/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/common/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/node_modules:/mnt/c/Users/marygao/project/node_modules:/mnt/c/Users/marygao/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/sdk/metricsadvisor/ai-metrics-advisor/node_modules/@azure/dev-tool/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/sdk/metricsadvisor/ai-metrics-advisor/node_modules/@azure/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/sdk/metricsadvisor/ai-metrics-advisor/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/sdk/metricsadvisor/node_modules:/mnt/c/Users/marygao/project/azure-sdk-for-js/sdk/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@azure/dev-tool/launch.js" "$@"
else
  exec node  "$basedir/../@azure/dev-tool/launch.js" "$@"
fi
